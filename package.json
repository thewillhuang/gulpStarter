    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.5.0",
    "bluebird": "^3.2.2",
    "ramda": "^0.19.1"
  },
  "devDependencies": {
    "babel-core": "^6.5.0",
    "babel-eslint": "^5.0.0-beta8",
    "babel-loader": "^6.2.2",
    "bower": "^1.7.7",
    "browser-sync": "^2.11.1",
    "concurrent-transform": "^1.0.0",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-loader": "^1.2.1",
