{
  "name": "chenpin",
  "version": "1.0.0",
  "description": "FDA for china",
  "homepage": "http://www.chenpin.org",
  "repository": {
    "type": "git",
    "url": "git://github.com/thewillhuang/chenpin.git"
  },
  "scripts": {
    "development": "NODE_ENV='development' gulp",
    "publish": "NODE_ENV='production' gulp build:production && gulp publish",
    "nuke": "rm -rf node_modules src/static/bower_components public; npm i; npm run prod; npm run dev;",
    "dev": "npm run development",
    "prod": "npm run production",
    "production": "NODE_ENV='production' gulp production",
    "postinstall": "npm run version",
    "version": "node -v",
    "bower": "bower install --config.interactive=false",
    "start": "npm run production && node index",
    "test": "mocha --recursive --require ./helper.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.5.0",
